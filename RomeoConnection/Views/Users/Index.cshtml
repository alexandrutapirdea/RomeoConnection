@model IEnumerable < RomeoConnection.Models.User >
@{
    ViewBag.Title = "Index";
}

<h2>List of users</h2>

<table class="table table-bordered table-hover">
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.firstName</td>
            <td>@user.lastName</td>
        </tr>
    }
    </tbody>
</table>

