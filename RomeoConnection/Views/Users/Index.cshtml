@model  IEnumerable < RomeoConnection.ViewModels.User >
@{
    ViewBag.Title = "Index";
}

<h2>List of users</h2>

<table class="table table-bordered table-hover">
    <thead>
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Location</th>
        <th>Job Title</th>
        <th>BirthDay</th>
        <th>Description</th>
    </tr>
    </thead>
    <tbody>
    @foreach (var user in Model)
    {
        <tr>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Location</td>
            <td>@user.JobTitle</td>
            <td>@user.Birthday</td>
            <td>@user.Description</td>

        </tr>
    }
    </tbody>
</table>

